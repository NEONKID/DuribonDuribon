    <LinearLayout
        android:id="@+id/lay_D"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:orientation="horizontal" />

    <LinearLayout
        android:id="@+id/lay_E"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:orientation="horizontal" />

    <LinearLayout
        android:id="@+id/lay_F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:orientation="horizontal" />

    <LinearLayout
        android:id="@+id/lay_G"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:orientation="horizontal" />

    <LinearLayout
        android:id="@+id/lay_H"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:orientation="horizontal" />

    <LinearLayout
        android:id="@+id/lay_I"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:orientation="horizontal" />

    <LinearLayout
        android:id="@+id/lay_J"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:orientation="horizontal" />

</LinearLayout>